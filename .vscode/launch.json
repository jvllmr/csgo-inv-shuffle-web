  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "${workspaceFolder}/csgo-inv-shuffle-web/flaskapp.py",
        "FLASK_ENV": "development",
        "STEAM_API_KEY": "${env:STEAM_API_KEY}"
      },
      "args": ["run", "--no-debugger"],
      "jinja": false
    },
    {
      "command": "npm start",
      "name": "React",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}/react/"
    }
  ]
}
